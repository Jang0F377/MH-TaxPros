{
  "version": 3,
  "name": "MH-TaxPro Gateway",
  "port": 8080,
  "timeout": "3000ms",
  "host": ["https://mhtaxpro.com/"],
  "endpoints": [
    {
      "endpoint": "/api/openings",
      "input_query_strings": ["*"],
      "input_headers": ["Access-Control-Allow-Origin"],
      "method": "GET",
      "backend": [
        {
          "method": "GET",
          "host": ["https://gcmtaxpros.fullslate.com/"],
          "url_pattern": "/api/v2/openings"
        }
      ]
    }
  ],
  "extra_config": {
    "security/cors": {
      "allow_origins": ["http://localhost:3000/"],
      "allow_headers": ["Origin", "Authorization", "Content-Type"],
      "expose_headers": ["Content-Length"],
      "allow_methods": ["GET"],
      "allow_credentials": true
    }
  }
}
